<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
  logicalFilePath="changelog-6.3">

  <changeSet author="author" id="changelog-6.3">
    
    <addColumn tableName="tc_devices">
      <column name="license" type="VARCHAR(128)">
        <constraints nullable="true"/>
      </column>
      <column name="vin" type="VARCHAR(128)">
        <constraints nullable="true"/>
      </column>
    </addColumn>

  </changeSet>

  <changeSet author="author" id="changelog-6.6.3-indexes-addition">
    <createIndex tableName="tc_devices" indexName="idx_devices_vin">
      <column name="vin" />
    </createIndex>
  </changeSet>


</databaseChangeLog>